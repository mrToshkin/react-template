  "scripts": {
    "dev": "webpack serve --open --config config/webpack/webpack.dev.js",
    "build": "yarn clean && yarn build:prod",
    "build:dev": "webpack --config config/webpack/webpack.dev.js",
    "build:prod": "webpack --config config/webpack/webpack.prod.js",
    "build:size": "NODE_ENV=production NODE_MODE=analyze webpack --config config/webpack/webpack.prod.js",
    "clean": "shx rm -rf dist",
    "ts:check": "tsc --noEmit --incremental",
    "test": "TZ=UTC jest",
    "test:coverage": "TZ=UTC jest --coverage",
    "all:check": "TZ=UTC yarn lint:check && yarn coverage:check && yarn stylelint:check && yarn ts:check && yarn prettier:check",
    "all:fix": "yarn lint:fix && yarn stylelint:fix && yarn prettier:fix",
    "lint:check": "eslint 'src/**' --cache --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "eslint 'src/**' --cache --ext .js,.jsx,.ts,.tsx src --fix",
    "stylelint:check": "stylelint src/**/*.{css,scss,sass}",
    "stylelint:fix": "stylelint src/**/*.{css,scss,sass} --fix",
    "prettier:check": "yarn prettier src/**/*.{ts,tsx} --check",
    "prettier:fix": "yarn prettier src/**/*.{ts,tsx} --write",
    "semantic-release": "semantic-release"
  },
